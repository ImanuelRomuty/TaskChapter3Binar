<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="20dp"
    tools:context=".ThirdFragment">

    <!-- TODO: Update blank fragment layout -->
    <EditText
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:id="@+id/et_usia"
        android:layout_marginBottom="10dp"
        android:paddingStart="10dp"
        android:hint="Masukkan Usia " />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:id="@+id/et_alamat"
        android:layout_marginTop="40dp"
        android:layout_below="@id/et_usia"
        android:layout_marginBottom="10dp"
        android:paddingStart="10dp"
        android:hint="Masukkan Alamat"
        android:inputType="text"
    />

    <EditText
        android:id="@+id/et_pekerjaan"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_below="@id/et_alamat"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="80dp"
        android:hint="Masukkan Pekerjaan"
        android:paddingStart="10dp"
/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="120dp"
        android:id="@+id/navigate_back_to_third_fragment_button"
        android:text="Go To Screen 3"
        android:layout_below="@id/et_pekerjaan"
        android:layout_centerHorizontal="true"
       />

</FrameLayout>